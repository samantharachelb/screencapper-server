{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    ["@babel/plugin-transform-typescript"],
    ["@babel/plugin-transform-modules-commonjs"],
    ["transform-class-properties"],
    ["module-resolver", {
      "extensions": [".ts"],
      "root": ["./src"],
      "alias": {
        "@src": "./src",
        "@utils": "./src/utils"
      }
    }]
  ]
}
